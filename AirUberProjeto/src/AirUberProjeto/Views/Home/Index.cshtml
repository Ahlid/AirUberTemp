@{
    ViewData["Title"] = "AirUber";
    Layout = "_NovoLayout";
}


<div class="w3-center w3-row-padding">
    <img style="height: 150px; margin-top: 10px" src="images/logo.png" />
     <h1 class="w3-center w3-text-blue-gray">AirUber</h1>
    <hr/>
    <div class="w3-col l4 w3">
        <div class="w3-col l12 w3-card-4 w3-margin"  style="height: 250px">
            <img style="height: 200px" class="w3-col l12" src="images/jet.jpg" alt="Norway">
            <div class="w3-container w3-center">
                <p style="margin-top: 200px ">Have the best jets in the world</p>
            </div>
        </div>
    </div>
    <div class="w3-col l4 w3">
        <div class="w3-col l12 w3-card-4 w3-margin" style="height: 250px">
            <img style="height: 200px" class="w3-col l12" src="images/7098575-World-map-of-airline-airplane-flight-travel-plans--Stock-Vector-routes.jpg" alt="Norway">
            <div class="w3-container w3-center">
                <p style="margin-top: 200px ">Travel to anywere</p>
            </div>
        </div>
    </div>
    <div class="w3-col l4 w3">
        <div class="w3-col l12 w3-card-4 w3-margin"  style="height: 250px">
            <img style="height: 200px" class="w3-col l12" src="images/timetravel.jpg" alt="Norway">
            <div class="w3-container w3-center">
                <p style="margin-top: 200px ">Save time and money</p>
            </div>
        </div>
    </div>
</div>
